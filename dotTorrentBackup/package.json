{
  "name": "dot-torrent",
  "version": "1.0.0",
  "description": "A bit torrent client built using electron",
  "main": "main.js",
  "scripts": {
    "start": "npm run build && electron .",
    "lint": "standard --fix",
    "build": "npm run sass:build && bundle exec jekyll build --source src --destination dist",
    "serve": "npm run sass:build && bundle exec jekyll serve --source src --destination src/_site",
    "sass:serve": "npm run sass:build -- --watch",
    "sass:build": "sass --style compressed src/stylesheets/sass/evaporate.scss:src/stylesheets/css/evaporate.min.css",
    "test": "mocha"
  },
  "repository": "https://github.com/playground-labs/torrentspace",
  "keywords": [
    "Electron",
    "dot-torrent",
    "dotTorrent",
    "dot Torrent",
    "dot_torrent",
    "DotTorrent",
    "bit-torrent-client"
  ],
  "author": "Punyasloka Parida",
  "license": "CC0-1.0",
  "devDependencies": {
    "eslint": "^5.13.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "mocha": "^6.0.2",
    "standard": "^12.0.1"
  },
  "dependencies": {
    "bencode": "^2.0.1",
    "electron": "^4.0.3",
    "electron-store": "^2.0.0",
    "standard-markdown": "^5.0.1"
  }
}
